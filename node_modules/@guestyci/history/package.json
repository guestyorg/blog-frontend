{
  "name": "@guestyci/history",
  "version": "5.1.1",
  "description": "Guesty navigation history",
  "dependencies": {
    "history": "^4.10.1"
  },
  "peerDependencies": {
    "@guestyci/foundation": "^9.0.1 >= || latest",
    "lodash": "^4.17.19"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guestyorg/history.git"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{jsx,js}": [
      "eslint --fix",
      "git add"
    ]
  },
  "jest": {
    "cache": false,
    "testURL": "http://localhost",
    "modulePathIgnorePatterns": [
      "<rootDir>/build"
    ],
    "globals": {
      "window": true
    }
  },
  "author": "Platform-FE",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/guestyorg/history/issues"
  },
  "homepage": "https://github.com/guestyorg/history#readme",
  "private": false,
  "main": "./index.js",
  "module": "./esm/index.js"
}