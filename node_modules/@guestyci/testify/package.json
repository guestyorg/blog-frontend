{
  "name": "@guestyci/testify",
  "version": "2.0.0",
  "description": "Client side testing library",
  "dependencies": {
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.3.5",
    "jest": "^24.9.0",
    "nock": "^11.4.0",
    "redux": "^4.0.5",
    "react-redux": "^7.2.2",
    "redux-mock-store": "^1.5.3",
    "redux-thunk": "^2.3.0",
    "react-jss": "^10.0.0"
  },
  "peerDependencies": {
    "react": ">=^16.8.6",
    "react-dom": ">=^16.8.6",
    "react-redux": ">=^7.1.3",
    "redux": ">=^4.0.4",
    "react-jss": ">=^10.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guestyorg/testify.git"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{jsx,js}": [
      "eslint --fix",
      "git add"
    ]
  },
  "jest": {
    "cache": false,
    "testURL": "http://localhost",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "./src/testSetup.js"
    ],
    "globals": {
      "window": true
    }
  },
  "author": "Platform-FE",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/guestyorg/testify/issues"
  },
  "homepage": "https://github.com/guestyorg/testify#readme",
  "private": false,
  "main": "./index.js",
  "module": "./esm/index.js"
}