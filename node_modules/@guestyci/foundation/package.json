{
  "name": "@guestyci/foundation",
  "version": "9.0.1",
  "description": "UI module for guesty components",
  "dependencies": {
    "@guestyci/memoize": "stable",
    "bootstrap": "^4.5.0",
    "cloudinary-react": "^1.5.1",
    "draft-js": "^0.11.6",
    "draft-convert": "^2.1.10",
    "jwt-decode": "^2.2.0",
    "react-dates": "^21.8.0",
    "react-day-picker": "^7.4.8",
    "react-intl-tel-input": "^8.0.4",
    "react-jss": "^10.3.0",
    "react-slick": "^0.26.1",
    "react-textarea-autosize": "^8.1.1",
    "react-transition-group": "^4.4.1",
    "react-virtualized": "^9.21.2",
    "reactstrap": "^8.5.1",
    "slick-carousel": "^1.8.1"
  },
  "peerDependencies": {
    "@guestyci/agni": "^2.0.0",
    "@guestyci/localize": "^1.2.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-jss": "^10.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guestyorg/foundation-ui.git"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn lint && yarn test:local",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{jsx,js}": [
      "eslint --fix",
      "git add"
    ]
  },
  "jest": {
    "cache": false,
    "testURL": "http://localhost",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css)$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(scss)$": "identity-obj-proxy"
    },
    "modulePathIgnorePatterns": [
      "<rootDir>/build"
    ],
    "globals": {
      "window": true
    }
  },
  "author": "Platform-FE",
  "sideEffects": [
    "*.scss"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/guestyorg/foundation-ui/issues"
  },
  "homepage": "https://github.com/guestyorg/foundation-ui#readme",
  "private": false,
  "main": "./index.js",
  "module": "./esm/index.js"
}