{
  "name": "@guestyci/redux-tools",
  "version": "1.1.4",
  "description": "Guesty redux tool module",
  "dependencies": {
    "lodash.isempty": "^4.4.0"
  },
  "peerDependencies": {
    "@guestyci/agni": "^1.0.9"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guestyorg/redux-tools.git"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{jsx,js}": [
      "eslint --fix",
      "git add"
    ]
  },
  "jest": {
    "cache": false,
    "testURL": "http://localhost",
    "globals": {
      "window": true
    }
  },
  "author": "Platform-FE",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/guestyorg/redux-tools/issues"
  },
  "homepage": "https://github.com/guestyorg/redux-tools#readme",
  "private": false,
  "main": "./index.js",
  "module": "./esm/index.js"
}