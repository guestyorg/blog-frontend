{
  "name": "renderkid",
  "version": "2.0.5",
  "description": "Stylish console.log for node",
  "main": "lib/RenderKid.js",
  "dependencies": {
    "css-select": "^2.0.2",
    "dom-converter": "^0.2",
    "htmlparser2": "^3.10.1",
