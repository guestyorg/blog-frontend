{
  "name": "lazystream",
  "version": "1.0.0",
  "description": "Open Node Streams on demand.",
  "homepage": "https://github.com/jpommerening/node-lazystream",
  "author": {
    "name": "Jonas Pommerening",
    "email": "jonas.pommerening@gmail.com",
